# ensure files always have Unix-style line endings on checkout.
.gitignore text eol=lf
.gitattributes text eol=lf
.gitmodules text eol=lf
*.sass text eol=lf
*.css text eol=lf
*.html text eol=lf
*.js text eol=lf
*.coffee text eol=lf
*.md text eol=lf
*.json text eol=lf
*.xml text eol=lf
*.map text eol=lf
*.svg text eol=lf
